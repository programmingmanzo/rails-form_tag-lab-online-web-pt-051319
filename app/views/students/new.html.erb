<h3>Student Form</h3>

<%= form_tag students_path do %>
    <label for="student_first_name">First name:</label><br>
    <%= text_field_tag :'student[first_name]' %><br>

    <label for="student_last_name">Last name:</label><br>
    <%= text_field_tag :'student[last_name]' %><br>

    <%= hidden_field_tag :authenticity_toekn, form_authenticity_token%>
    <%= submit_tag "Submit Student" %>
<% end %>